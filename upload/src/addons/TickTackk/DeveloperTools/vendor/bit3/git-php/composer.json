{
    "name": "bit3/git-php",
    "description": "Easy to use GIT wrapper for PHP.",
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "psr/log": "~1.0",
        "symfony/process": "~2.3 | ~3.0"
    },
    "require-dev": {
        "symfony/filesystem": "~2.3 | ~3.0",
        "phpcq/all-tasks": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\GitPhp\\": "src",
            "Bit3\\GitPhp\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev",
            "dev-develop": "1.5.x-dev"
        }
    }
}
